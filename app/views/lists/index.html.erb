<body>
    <p id="notice"></p>

<h1>Books</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.title %></td>
        <td><%= list.body %></td>
        <td><a class="" href="">Show</a></td>
        <td><a class="" href="">Edit</a></td>
        <td><a data-confirm="Are you sure?" class="destroy_83936" rel="nofollow" data-method="delete" href="https://bookers-level1.herokuapp.com/books/83936">Destroy</a></td>
      </tr>
    <% end %>
  </tbody>
</table>


<h2>New book</h2>
<%= form_with model: @list, method: :post do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>